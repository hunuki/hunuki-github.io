<%= form_for :post, url: posts_path do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>
    <p>
      <%= f.label :body %><br>
      <%= f.text_area :body %>
</p>
    <p>
      <%= f.submit %>
</p> <% end %>
<%= link_to 'Back', posts_path %>
